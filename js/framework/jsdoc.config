{
    "recurseDepth": 10,
    "source": {
        "include":["README.md"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts" : {
        "destination": "./Doc/"
    },
    "plugins": ["plugins/markdown"],
    "markdown": {
        "tags": ["class"]
    }

}

